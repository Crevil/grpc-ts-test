{
  "name": "grpc-ts-test",
  "private": true,
  "version": "1.0.0",
  "description": "Test project for the gRPC typescript definitions",
  "main": "index.ts",
  "scripts": {
    "test": "tsc --strict --noEmit index.ts",
    "prettier": "prettier --config .prettierrc --write \"index.ts\"",
    "lint": "tslint index.ts node_modules/grpc/index.d.ts",
    "format": "npm run prettier && npm run lint"
  },
  "keywords": [
    "typescript",
    "grpc"
  ],
  "author": "Crevil <bjoern.soerensen@gmail.com> (https://github.com/Crevil)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "8.0.31",
    "prettier": "1.7.4",
    "tslint": "5.7.0",
    "tslint-config-prettier": "1.5.0",
    "tslint-plugin-prettier": "1.3.0",
    "typescript": "2.5.3"
  },
  "dependencies": {
    "grpc": "1.6.0",
    "protobufjs": "6.8.0"
  }
}
